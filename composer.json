{
  "require-dev": {
    "mediawiki/mediawiki-codesniffer": "23.0.0",
    "phpunit/phpunit": "4.8.36 || ^6.5",
    "jakub-onderka/php-parallel-lint": "0.9.2",
    "mediawiki/minus-x": "0.3.1"
  },
  "scripts": {
    "lint": "parallel-lint --exclude vendor",
    "phpcs": "phpcs -p -s",
    "fix": "phpcbf",
    "test": [
      "composer lint",
      "composer phpcs",
      "minus-x check"
    ]
  }
}
